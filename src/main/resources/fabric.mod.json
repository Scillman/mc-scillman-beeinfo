{
    "schemaVersion": 1,
    "id": "beeinfo",
    "version": "${version}",
    "name": "BeeInfo",
    "description": "Rewritten Beenfo mod of Giselbaer for Minecraft versions 1.21.",
    "authors": [
        "Giselbaer",
        "Scillman"
    ],
    "contact": {
        "homepage": "https://github.com/Scillman/mc-scillman-beeinfo/",
        "sources": "https://github.com/Scillman/mc-scillman-beeinfo/",
        "issues": "https://github.com/Scillman/mc-scillman-beeinfo/issues/"
    },
    "license": "MIT",
    "icon": "assets/beeinfo/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.github.scillman.minecraft.beeinfo.BeeInfoServer"
        ],
        "client": [
            "com.github.scillman.minecraft.beeinfo.BeeInfoClient"
        ],
        "modmenu": [
            "com.github.scillman.minecraft.beeinfo.gui.OptionsMenu"
        ]
    },
    "mixins": [
        "beeinfo.mixins.json",
        {
            "config": "beeinfo.client.mixins.json",
